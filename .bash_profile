export BASH_SILENCE_DEPRECATION_WARNING=1

# prompt
PS1=' \W > '

# edit file in Text Mate
alias edit='open -a TextMate'

# clear screen
alias cls='clear'

# clone a repo
alias gcl='git clone'

# youtube downloader
alias ydl='youtube-dl'
alias ydl-139='youtube-dl -f 139'
alias ydl-140='youtube-dl -f 140'

# neofetch
alias neo='neofetch'

# GNU Make
alias foo='make && ./bin/main'
